var mongoose = require( 'mongoose' );

// create Donation Bucket schema

var donationBucketSchema = new mongoose.Schema({
    _id: {
        $oid: String
    },
    title: String,
    description: String,
    feedImageUrl: String,
    vendorId: Number,
    handle: String
});

// create model
exports.DonationBucket = mongoose.model('donation-bucket', donationBucketSchema);
